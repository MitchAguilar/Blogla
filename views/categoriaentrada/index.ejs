<table class="mdl-data-table mdl-js-data-table mdl-shadow--2dp">
  <thead>
    <tr>
      <th class="mdl-data-table__cell--non-numeric">#</th>
      <th>Categoria</th>
      <th>Creado</th>
      <th colspan="3">Opciones</th>
    </tr>
  </thead>
  <tbody>
    <% for (var i = 0; i < categoriasentradas.length; i++) { %>
      <tr>
        <th class="mdl-data-table__cell--non-numeric">
          <%= (i+1) %>
        </th>
        <td>
          <%= categoriasentradas[i].nombre_categoria %>
        </td>
        <td>
          <%= moment(categoriasentradas[i].createdAt).format('LLS') %>
        </td>
        <td>
          <a href="/categoriaentrada/<%= categoriasentradas[i].id %>" target="_blank">
            Ver
          </a>
        </td>
      </tr>
    <% } %>
  </tbody>
</table>
